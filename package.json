{
  "name": "client",
  "productName": "Clicker Game",
  "version": "1.0.0",
  "description": "Простой кликер на Electron",
  "homepage": "https://example.com",
  "main": "client/src/main.js",
  "author": {
    "name": "Denis Artem Pavel Victor",
    "email": "ryapolov.den@mail.ru"
  },
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run server\" \"electron . --no-sandbox --disable-gpu\"",
    "server": "python3 server/server.py",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "electron-builder",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "concurrently": "^9.1.2",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "util": "^0.12.5"
  },
  "build": {
    "appId": "com.example.clicker",
    "files": [
      "src/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "jest": {
      "testEnvironment": "jsdom",
      "setupFiles": [
        "./testSetup.js"
      ],
      "silent": true,
      "verbose": false
    }
  }
}
